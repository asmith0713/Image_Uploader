{
  "name": "telebox",
  "version": "1.0.0",
  "description": "TeleBox - Your Telegram storage box. MERN stack image uploader that sends images to Telegram",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": ["mern", "react", "express", "telegram", "image-uploader"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.1",
    "multer": "^1.4.5-lts.1",
    "node-telegram-bot-api": "^0.64.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^2.0.22"
  },
  "engines": {
    "node": "20.10.0"
  }
}
